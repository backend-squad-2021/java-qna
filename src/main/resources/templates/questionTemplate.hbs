<ul class="list">
  {{#questions}}
    <li>
      <div class="wrap">
        <div class="main">
          <strong class="subject">
            <a href="/questions/{{id}}">{{title}}</a>
          </strong>
          <div class="auth-info">
            <i class="icon-add-comment"></i>
            <span class="time">{{lastModifiedDateTime}}</span>
            <a class="author">{{user.userId}}</a>
          </div>
          <div class="reply" title="댓글">
            <i class="icon-reply"></i>
            <span class="point"></span>
          </div>
        </div>
      </div>
    </li>
  {{/questions}}
</ul>
